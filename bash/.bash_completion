# Git bash completion
[[ -f /usr/share/git/completion/git-completion.bash ]] && . /usr/share/git/completion/git-completion.bash

# Tomb bash completion
[[ -f ~/.config/tomb/tomb-completion.bash ]] && . ~/.config/tomb/tomb-completion.bash

# TimeWarrior completion
[[ -f ~/external/timew-bashcompletion/timew ]] && . ~/external/timew-bashcompletion/timew

