!URxvt*font: xft:Inconsolata:size=10
URxvt*font: xft:Inconsolata Nerd Font Mono:size=10
URxvt.scrollBar: false
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
! Setting transparent background
URxvt*depth: 32
URxvt*background: [80]#000000

xterm*faceName: Inconsolata
xterm*faceSize: 10
xterm*renderFont: true

! Color Scheme from terminal.sexy
! google.dark

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc342b
*.color9:       #cc342b

! green
*.color2:       #198844
*.color10:      #198844

! yellow
